@model IEnumerable<CDBBank.Models.FD>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FdInvMon)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Month)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Period)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.n)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FdMAmount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FdInMoney)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FdInvMon)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Month)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Period)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.n)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FdMAmount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FdInMoney)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FdId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FdId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FdId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
