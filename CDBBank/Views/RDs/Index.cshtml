@model IEnumerable<CDBBank.Models.RD>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RdInvMon)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Time)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Period)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.n)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FdMatureAmount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FdInMoney)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RdInvMon)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Time)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Period)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.n)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FdMatureAmount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FdInMoney)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RdId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.RdId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.RdId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
